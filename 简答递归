public class test
{
	public static void main(String[]args)
	{
		java.util.Scanner s=new java.util.Scanner(System.in);
		int a=s.nextInt();int b=s.nextInt();
		System.out.println(sum(a,b));
	}
	public static int sum(int a,int b)
		{
		int i=b;
		if(i==a)
			{
			return a; 
			}
		else
			{
			return sum(a,i-1)*i;
			}
		}
}
